<!--buildnew.wxml-->
<view class="page">
  <view class="page__hd"></view>
  <view class="page__bd">

    <view class="self-container">
      <!--新增仓库-->
      <view class="weui-cells__title">新增仓库</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入仓库名" bindinput="getinput" />
                </view>
            </view>
        </view>
    </view>
    <!--确认按钮-->
    <button class="weui-btn self-btn" type="primary" bindtap="submit" plain='true'>确认提交</button>
    <view class="weui-footer weui-footer_fixed-bottom">
    <view class="weui-footer__text">Crossin的编程教室</view>
  </view>
  </view>
</view>